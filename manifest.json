{
    "manifest_version" : 3,
    "name" : "View Only Tab",
    "description": "Extension to make a tab view only",
    "version": "0.0.1",
    "action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "history",
        "tabs",
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}